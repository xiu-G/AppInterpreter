    public abstract oauth.signpost.http.HttpRequest sign(java.lang.Object) throws oauth.signpost.exception.OAuthMessageSignerException, oauth.signpost.exception.OAuthExpectationFailedException, oauth.signpost.exception.OAuthCommunicationException;

