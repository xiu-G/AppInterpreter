public interface  twitter4j.Status extends java.lang.Object implements twitter4j.TwitterResponse, java.io.Serializable
{

    public abstract java.util.Date getCreatedAt();

    public abstract twitter4j.GeoLocation getGeoLocation();

    public abstract long getId();

    public abstract java.lang.String getInReplyToScreenName();

    public abstract long getInReplyToStatusId();

    public abstract int getInReplyToUserId();

    public abstract twitter4j.Status getRetweetedStatus();

    public abstract java.lang.String getSource();

    public abstract java.lang.String getText();

    public abstract twitter4j.User getUser();

    public abstract boolean isFavorited();

    public abstract boolean isRetweet();

    public abstract boolean isTruncated();
}
