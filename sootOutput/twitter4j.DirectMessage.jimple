public interface  twitter4j.DirectMessage extends java.lang.Object implements twitter4j.TwitterResponse, java.io.Serializable
{

    public abstract java.util.Date getCreatedAt();

    public abstract int getId();

    public abstract twitter4j.User getRecipient();

    public abstract int getRecipientId();

    public abstract java.lang.String getRecipientScreenName();

    public abstract twitter4j.User getSender();

    public abstract int getSenderId();

    public abstract java.lang.String getSenderScreenName();

    public abstract java.lang.String getText();
}
