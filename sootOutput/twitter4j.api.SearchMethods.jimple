public interface  twitter4j.api.SearchMethods extends java.lang.Object
{

    public abstract twitter4j.Trends getCurrentTrends() throws twitter4j.TwitterException;

    public abstract twitter4j.Trends getCurrentTrends(boolean) throws twitter4j.TwitterException;

    public abstract java.util.List getDailyTrends() throws twitter4j.TwitterException;

    public abstract java.util.List getDailyTrends(java.util.Date, boolean) throws twitter4j.TwitterException;

    public abstract twitter4j.Trends getTrends() throws twitter4j.TwitterException;

    public abstract java.util.List getWeeklyTrends() throws twitter4j.TwitterException;

    public abstract java.util.List getWeeklyTrends(java.util.Date, boolean) throws twitter4j.TwitterException;

    public abstract twitter4j.QueryResult search(twitter4j.Query) throws twitter4j.TwitterException;
}
